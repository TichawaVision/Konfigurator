<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane id="AnchorPane" prefHeight="880.0" prefWidth="1200.0" stylesheets="@style.css"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="de.tichawa.cis.config.controller.CalculationController">
    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0">
        <MenuBar id="Menu" prefHeight="20.0" GridPane.columnSpan="2">
            <Menu fx:id="fileMenu">
                <MenuItem fx:id="printMenuItem" onAction="#print"/>
            </Menu>
            <Menu fx:id="languageMenu">
                <MenuItem fx:id="switchLanguageMenuItem" onAction="#switchLang"/>
            </Menu>
        </MenuBar>
        <Label id="Header" fx:id="headerLabel" maxWidth="Infinity" text="Header"/>
        <TableView id="Electronics" fx:id="electronicsTableView" maxHeight="Infinity" VBox.vgrow="SOMETIMES"/>
        <TableView id="ElectSubTotals" fx:id="electronicsSubTotalsTableView" maxHeight="30.0" VBox.vgrow="NEVER"/>
        <TableView id="Mechanics" fx:id="mechanicsTableView" maxHeight="Infinity" VBox.vgrow="SOMETIMES"/>
        <TableView id="MechaSubTotals" fx:id="mechanicsSubTotalsTableView" maxHeight="30.0" VBox.vgrow="NEVER"/>
        <TableView id="Totals" fx:id="totalsTableView" maxHeight="Infinity" minHeight="350.0" VBox.vgrow="ALWAYS"/>
    </VBox>
</AnchorPane>
