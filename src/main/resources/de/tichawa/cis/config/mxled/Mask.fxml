          <RowConstraints maxHeight="15.0" minHeight="15.0" prefHeight="15.0" vgrow="SOMETIMES"/>
          <RowConstraints maxHeight="15.0" minHeight="15.0" prefHeight="15.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label style="visibility: hidden;" text="Max Line Rate:"/>
        <Label fx:id="MaxLineRate" style="visibility: hidden;" text="0 kHz" GridPane.columnIndex="1"/>
        <Label style="visibility: hidden;" text="Curr. Line Rate:" GridPane.rowIndex="1"/>
        <Label fx:id="CurrLineRate" style="visibility: hidden;" text="0 kHz" GridPane.columnIndex="1"
               GridPane.rowIndex="1"/>
        <Slider fx:id="SelLineRate" min="100" style="visibility: hidden;" GridPane.columnSpan="2"
                GridPane.rowIndex="2"/>
        <Label style="visibility: hidden;" text="Transport Speed:" GridPane.columnIndex="2"/>
        <Label fx:id="Speedmms" style="visibility: hidden;" text="0 mm/s" GridPane.columnIndex="3"/>
        <Label fx:id="Speedms" style="visibility: hidden;" text="0 m/s" GridPane.columnIndex="3"
               GridPane.rowIndex="1"/>
        <Label fx:id="Speedmmin" style="visibility: hidden;" text="0 m/min" GridPane.columnIndex="3"
               GridPane.rowIndex="2"/>
        <Label fx:id="Speedips" style="visibility: hidden;" text="0 ips" GridPane.columnIndex="3"
               GridPane.rowIndex="3"/>
      </GridPane>
    </TitledPane>
    <TitledPane collapsible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Light"
                GridPane.columnSpan="3" GridPane.fillHeight="true" GridPane.fillWidth="true" GridPane.rowIndex="2">
      <GridPane alignment="CENTER" hgap="5.0" vgap="5.0" GridPane.columnSpan="3" GridPane.rowIndex="2">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="130.0" minWidth="130.0" prefWidth="130.0"/>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="160.0" minWidth="160.0" prefWidth="160.0"/>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="130.0" minWidth="130.0" prefWidth="130.0"/>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="160.0" minWidth="160.0" prefWidth="160.0"/>
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
          <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label text="Light Source:"/>
        <ComboBox fx:id="InternalLightSource" disable="true" maxWidth="160.0" minWidth="160.0" prefHeight="25.0"
                  prefWidth="160.0" promptText="One sided" GridPane.columnIndex="1">
          <items>
            <FXCollections fx:factory="observableArrayList">
              <String fx:value="None"/>
              <String fx:value="One sided"/>
              <String fx:value="Two sided"/>
              <String fx:value="Two sided plus Coax"/>
              <String fx:value="Coax"/>
            </FXCollections>
          </items>
        </ComboBox>
        <Label text="Light Color:" GridPane.rowIndex="1"/>
        <ComboBox fx:id="InternalLightColor" maxWidth="160.0" minWidth="160.0" prefWidth="160.0" promptText="Red"
                  GridPane.columnIndex="1" GridPane.rowIndex="1">
          <items>
            <FXCollections fx:factory="observableArrayList">
              <String fx:value="Red"/>
              <String fx:value="Green"/>
              <String fx:value="Blue"/>
              <String fx:value="IR"/>
              <String fx:value="Yellow"/>
              <String fx:value="White"/>
            </FXCollections>
          </items>
        </ComboBox>
        <Label style="visibility: hidden;" text="External Light Source:" GridPane.columnIndex="2"/>
        <ComboBox fx:id="ExternalLightSource" maxWidth="160.0" minWidth="160.0" prefWidth="160.0"
                  promptText="One sided" style="visibility: hidden;" GridPane.columnIndex="3">
          <items>
            <FXCollections fx:factory="observableArrayList">
              <String fx:value="None"/>
              <String fx:value="One sided"/>
              <String fx:value="Two sided"/>
              <String fx:value="Coax"/>
              <String fx:value="One sided plus Coax"/>
            </FXCollections>
          </items>
        </ComboBox>
        <Label style="visibility: hidden;" text="External Light Color:" GridPane.columnIndex="2"
               GridPane.rowIndex="1"/>
        <ComboBox fx:id="ExternalLightColor" maxWidth="160.0" minWidth="160.0" prefWidth="160.0" promptText="Red"
                  style="visibility: hidden;" GridPane.columnIndex="3" GridPane.rowIndex="1">
          <items>
            <FXCollections fx:factory="observableArrayList">
              <String fx:value="Red"/>
              <String fx:value="Green"/>
              <String fx:value="Blue"/>
              <String fx:value="IR"/>
              <String fx:value="Yellow"/>
              <String fx:value="White"/>
            </FXCollections>
          </items>
        </ComboBox>
      </GridPane>
    </TitledPane>
    <HBox alignment="CENTER" spacing="5.0" GridPane.columnSpan="3" GridPane.rowIndex="3">
      <Label style="visibility: hidden;" text="Interface:"/>
      <ComboBox fx:id="Interface" promptText="CameraLink" style="visibility: hidden;">
        <items>
          <FXCollections fx:factory="observableArrayList">
            <String fx:value="CameraLink"/>
            <String fx:value="GigE"/>
          </FXCollections>
        </items>
      </ComboBox>
    </HBox>
    <HBox alignment="CENTER" spacing="5.0" GridPane.columnSpan="3" GridPane.rowIndex="4">
      <Label style="visibility: hidden;" text="Cooling:"/>
      <ComboBox fx:id="Cooling" promptText="Int. Forced Air (Default)" style="visibility: hidden;">
        <items>
          <FXCollections fx:factory="observableArrayList">
            <String fx:value="None"/>
            <String fx:value="Int. Forced Air (Default)"/>
            <String fx:value="Ext. Forced Air"/>
            <String fx:value="Passive Air"/>
            <String fx:value="Liquid Cooling"/>
          </FXCollections>
        </items>
      </ComboBox>
    </HBox>
